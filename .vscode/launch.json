  "version": "0.2.0",
  "configurations": [
    {
      "name": "Shell Lab / myint",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/Shell Lab/myint",
      "args": [
        // will sleep 3 seconds, and sent SIGINT to itself.
        "3"
      ],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/Shell Lab",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "为 gdb 启用整齐打印",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "将反汇编风格设置为 Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },
